drug_list:
    find:
        token:
        - "MEDICAL DECISION MAKING"
        - "[0-9]+"
    refine:
        dose_unit:
            window:
                tokens_following: 5
            find:
                token: 'mg'
            output:
                drug_dose_unit: '[0-9]+'
                drug_name: "jaja"

            action:
                - "flush"
                - "clear"
